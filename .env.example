
# DEBUG
# Enable debug mode
# Example: true
DEBUG=true

# NODE ENV
# The environment the server is running in
# Example: development
NODE_ENV=development

# JWT Secret
# Used to sign and verify JWT tokens
# Example: test
JWT_SECRET=test

# JWT EXPIRE
# The time in seconds that a JWT token will expire
# Example: 86400
JWT_EXPIRE=86400

# Web Port
# The port the web server will listen on
# Example: 3000
WEB_PORT=3000

# MYSQL Host
# The host of the MySQL server
# Example: localhost
MYSQL_HOST=localhost

# MYSQL Port
# The port of the MySQL server
# Example: 3306
MYSQL_PORT=3306

# MYSQL User
# The user to connect to the MySQL server
# Example: root
MYSQL_USER=root

# MYSQL Password
# The password to connect to the MySQL server
# Example: password
MYSQL_PASSWORD=password

# MYSQL Database
# The database to connect to on the MySQL server
# Example: test
MYSQL_DATABASE=test

# S3 Region
# The region of the S3 bucket
# Example: us-east-1
S3_REGION=region

# S3 Endpoint Protocol
# The protocol of the S3 endpoint
# Example: https://
S3_ENDPOINT_PROTOCOL=https://

# S3 Endpoint URL
# The URL of the S3 endpoint
# Example: s3.amazonaws.com
S3_ENDPOINT_URL=url

# S3 Access Key ID
# The access key ID for the S3 bucket
# Example: access_key_id
S3_ACCESS_KEY_ID=access_key_id

# S3 Secret Access Key
# The secret access key for the S3 bucket
# Example: secret_access_key
S3_SECRET_ACCESS_KEY=secret_access_key

# S3 Bucket Name
# The name of the S3 bucket
# Example: bucket_name
S3_BUCKET_NAME=bucket_name

# S3 CDN URL
# The URL of the S3 CDN
# Example: https://cdn.example.com
S3_CDN_URL=url
